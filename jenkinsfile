pipeline {
    agent any
    tools {nodejs "NODEJS"}
    stages {
        stage('Build') {
            steps {
                bat 'npm install'
            }
        }
        stage('Deliver') {
            steps {
                bat ' ./jenkins/scripts/deliver.bat'
                bat ' ./jenkins/scripts/kill.bat'
                bat './jenkins/scripts/deliver.bat'
                input message: 'Finibated using the web site? (Click "Proceed" to continue)'
                bat './jenkins/scripts/kill.bat'
            }
        }
    }
}